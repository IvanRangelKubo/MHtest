{% if section.blocks != blank %}

  <div class="seccgridcuatro">
    <div class="custom-container grid">
      <div id="w-node-_86f5b5bb-0c87-714b-5702-ba46283f2e9f-bfeb76ea" class="w-layout-layout stackgrid wf-layout-layout">

        {% for block in section.blocks %}
          <div id="w-node-_86f5b5bb-0c87-714b-5702-ba46283f2ea0-bfeb76ea" class="w-layout-cell">
            <a href="{{ block.settings.url }}" class="linkgrid w-inline-block">
              <img src="{{ block.settings.image |  image_url }}"  alt="{{ block.settings.image }}" class="imggrid">
            </a>
          </div>
        {% endfor %}
        
      </div>
    </div>
  </div>

{% else %}

  <div class="seccgridcuatro">
